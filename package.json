{
  "name": "new-launch-api",
  "version": "1.0.0",
  "description": "for newlaunch API",
  "main": "app.js",
  "scripts": {
    "precommit": "lint-staged",
    "format": "prettier --single-quote --trailing-comma es5 --write \"src/**/*.js\"",
    "test": "mocha ./test/unit --require babel-register --require babel-polyfill",
    "test:unit": "cross-env NODE_ENV=local mocha --require babel-register --require babel-polyfill ./tests/unit/*.spec.js --exit",
    "test:integration": "cross-env NODE_ENV=local mocha ./tests/integration/*.spec.js --require babel-register --require babel-polyfill --exit",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=local NODE_ICU_DATA=./node_modules/full-icu node dist/bin/server.js",
    "flow": "flow",
    "eslint": "eslint ./src --cache --ignore-pattern .gitignore",
    "eslint:fix": "eslint --fix ./src",
    "build": "babel src/ -d dist/ --copy-files"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma es5 --write",
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^3.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "express-decorators": "^1.0.0",
    "flow-bin": "^0.66.0",
    "full-icu": "^1.2.1",
    "get-port": "^3.2.0",
    "mocha": "^5.0.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.15.1",
    "prettier": "^1.11.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "sinon": "^4.2.2",
    "sinon-chai": "^3.0.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "moment": "^2.21.0",
    "express": "^4.16.2",
    "lodash": "^4.17.5",
    "mysql2": "^1.5.2",
    "php-serialization": "0.0.4",
    "redis": "^2.8.0",
    "sequelize": "^4.35.2",
    "solr-client": "^0.7.0",
    "sprintf-js": "^1.1.1"
  }
}
